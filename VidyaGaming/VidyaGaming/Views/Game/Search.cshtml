@using VidyaGaming.DAL
@model List<Game>

<form method="post">
    <input type="text" name="Name" />
    <input type="submit" method="post" />
</form>

@if (Model != null){
    @foreach (var game in Model)
    {
    
               <h1>@game.Name</h1>
               <img src=@game.Image />
    
    }
}


	






